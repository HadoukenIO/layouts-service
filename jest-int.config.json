{
    "rootDir": "test",
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/../node_modules/ts-jest"
    },
    "testRunner": "jest-circus/runner",
    "modulePaths": [
      "<rootDir>/../node_modules"
    ],
    "setupFilesAfterEnv": ["<rootDir>/provider/utils/setup.ts"],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
}