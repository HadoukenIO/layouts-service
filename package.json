    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.5.1",
    "execa": "^0.10.0",
    "expose-loader": "^0.7.5",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "gts": "^0.7.1",
    "hadouken-js-adapter": "^0.32.1-alpha.3",
