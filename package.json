  "version": "1.0.0",
  "devDependencies": {
    "@types/jest": "^23.3.0",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.6.0",
    "css-loader": "^0.28.10",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
