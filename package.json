    "fs-extra": "^5.0.0",
    "gts": "^0.6.0",
    "hadouken-js-adapter": "^0.32.1-alpha.5",
    "jest": "^23.4.1",
    "mini-css-extract-plugin": "^0.4.0",
    "npm": "^5.7.1",
    "openfin-config-builder": "^1.0.9",
    "openfin-launcher": "^1.4.2",
    "openfin-layouts": "0.0.2",
    "style-loader": "^0.20.2",
    "ts-jest": "^23.0.1",
    "ts-loader": "^4.3.1",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.9.1",
    "url-loader": "^0.6.2",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13"
  },
  "dependencies": {
    "@types/openfin": "^29.0.0",
    "@types/uuid": "^3.4.3",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "build": "webpack && node build.js",
    "webpack": "webpack --watch",
    "lint": "tslint --project .",
    "start": "concurrently --kill-others \"webpack --mode development --watch\" \"sleep 5 && node server.js\"",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "tslint --fix --project .",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/src/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
