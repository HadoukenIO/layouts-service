{
    "devtools_port": 9091,
    "startup_app": {
        "name": "Layout-Manager",
        "description": "OpenFin Layouts Managing Service",
        "url": "$PLACEHOLDER",
        "uuid": "Layout-Manager",
        "autoShow": true,
        "showTaskbarIcon": true,
        "defaultHeight": 300,
        "defaultWidth": 400,
        "_experimental": {
            "v2Api": true
        }
    },
    "exports": {
        "name": "layoutsManager",
        "supportedVersions": [
            "0.0.1"
        ]
    },
    "runtime": {
        "arguments": "--v=1 --enable-crash-reporting",
        "version": "$PLACEHOLDER"
    }
}
