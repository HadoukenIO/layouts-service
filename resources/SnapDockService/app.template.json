{
    "devtools_port": 9090,
    "startup_app": {
        "name": "Layouts-Manager",
        "description": "OpenFin Layouts Managing Service",
        "url": "$PLACEHOLDER",
        "uuid": "Layouts-Manager",
        "autoShow": false,
        "showTaskbarIcon": false,
        "defaultHeight": 400,
        "defaultWidth": 500,
        "_experimental": {
            "v2Api": true
        }
    },
    "exports": {
        "name": "layouts",
        "supportedVersions": [
            "0.0.1"
        ]
    },
    "runtime": {
        "arguments": "--v=1 --enable-crash-reporting",
        "version": "$PLACEHOLDER"
    }
}